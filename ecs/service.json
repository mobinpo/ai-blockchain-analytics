{
  "serviceName": "ai-blockchain-analytics",
  "cluster": "ai-blockchain-analytics-cluster",
  "taskDefinition": "ai-blockchain-analytics",
  "desiredCount": 2,
  "launchType": "FARGATE",
  "platformVersion": "LATEST",
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": [
        "subnet-12345678",
        "subnet-87654321"
      ],
      "securityGroups": [
        "sg-ai-blockchain-app"
      ],
      "assignPublicIp": "ENABLED"
    }
  },
  "loadBalancers": [
    {
      "targetGroupArn": "arn:aws:elasticloadbalancing:us-east-1:YOUR_ACCOUNT:targetgroup/ai-blockchain-analytics/abcd1234",
      "containerName": "laravel-app",
      "containerPort": 8000
    }
  ],
  "serviceRegistries": [
    {
      "registryArn": "arn:aws:servicediscovery:us-east-1:YOUR_ACCOUNT:service/srv-ai-blockchain-analytics"
    }
  ],
  "deploymentConfiguration": {
    "maximumPercent": 200,
    "minimumHealthyPercent": 50,
    "deploymentCircuitBreaker": {
      "enable": true,
      "rollback": true
    }
  },
  "deploymentController": {
    "type": "ECS"
  },
  "enableExecuteCommand": true,
  "enableLogging": true,
  "propagateTags": "SERVICE",
  "tags": [
    {
      "key": "Environment",
      "value": "production"
    },
    {
      "key": "Project",
      "value": "ai-blockchain-analytics"
    },
    {
      "key": "ManagedBy",
      "value": "ECS"
    }
  ]
}