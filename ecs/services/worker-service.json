{
  "serviceName": "ai-blockchain-analytics-worker",
  "cluster": "ai-blockchain-analytics-cluster",
  "taskDefinition": "ai-blockchain-analytics-worker",
  "desiredCount": 2,
  "launchType": "FARGATE",
  "platformVersion": "LATEST",
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": [
        "subnet-xxxxxxxxx",
        "subnet-yyyyyyyyy",
        "subnet-zzzzzzzzz"
      ],
      "securityGroups": [
        "sg-worker-xxxxxxxxx"
      ],
      "assignPublicIp": "DISABLED"
    }
  },
  "deploymentConfiguration": {
    "deploymentCircuitBreaker": {
      "enable": true,
      "rollback": true
    },
    "maximumPercent": 200,
    "minimumHealthyPercent": 50,
    "alarms": {
      "alarmNames": [
        "ai-blockchain-analytics-worker-high-cpu",
        "ai-blockchain-analytics-worker-queue-size"
      ],
      "enable": true,
      "rollback": true
    }
  },
  "capacityProviderStrategy": [
    {
      "capacityProvider": "FARGATE_SPOT",
      "weight": 1,
      "base": 1
    }
  ],
  "enableExecuteCommand": true,
  "enableLogging": true,
  "tags": [
    {
      "key": "Environment",
      "value": "production"
    },
    {
      "key": "Application",
      "value": "ai-blockchain-analytics"
    },
    {
      "key": "Component",
      "value": "worker-service"
    }
  ],
  "propagateTags": "SERVICE"
}
