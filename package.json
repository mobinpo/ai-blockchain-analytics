{
    "name": "ai-blockchain-analytics",
    "version": "1.0.0",
    "description": "AI-powered blockchain analytics platform with Laravel 12 + Octane",
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "keywords": ["laravel", "octane", "vue", "inertia", "blockchain", "analytics", "ai"],
    "repository": {
        "type": "git",
        "url": "https://github.com/mobin/ai-blockchain-analytics.git"
    },
    "scripts": {
        "build": "vite build && vite build --ssr",
        "dev": "vite",
        "lint": "eslint resources/js --ext .vue,.js --fix",
        "lint:check": "eslint resources/js --ext .vue,.js",
        "format": "prettier --write resources/js",
        "test": "vitest",
        "test:ci": "vitest run --coverage",
        "type-check": "vue-tsc --noEmit"
    },
    "devDependencies": {
        "@inertiajs/vue3": "^2.0.0",
        "@tailwindcss/forms": "^0.5.3",
        "@vitejs/plugin-vue": "^5.0.0",
        "@vue/server-renderer": "^3.4.0",
        "autoprefixer": "^10.4.12",
        "axios": "^1.8.2",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^1.0.0",
        "postcss": "^8.4.31",
        "tailwindcss": "^3.2.1",
        "vite": "^6.0.0",
        "vue": "^3.4.0"
    }
}
